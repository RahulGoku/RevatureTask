package operator;
import java.util.*;

public class Loop_Operator {
public static void main(String[] args) {
	int a,b;
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter the Value of a and b : ");
	a=sc.nextInt();
	b=sc.nextInt();
	Operator_test o = new Operator_test();
	o.compareNumber(a, b);
	o.logicalOperator(a, b);
	o.shiftNumber(a, b);
	
}
}
